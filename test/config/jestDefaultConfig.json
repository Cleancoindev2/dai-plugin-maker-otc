{
  "rootDir": "../../",
  "coverageReporters": [
    "json",
    "lcov",
    "text-summary"
  ],
  "collectCoverageFrom": [
    "src/*.js"
  ],
  "globalSetup": "<rootDir>/test/config/setup-global.js",
  "roots": [
    "src",
    "test"
  ],
  "setupFilesAfterEnv": ["<rootDir>/test/config/setup-test.js"]
}